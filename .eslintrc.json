{
    "env": {
        "browser": true,
        "es2021": true,
        "commonjs": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-use-before-define": ["error", {
          "functions": true,
          "classes": true,
          "variables": true,
          "allowNamedExports": false
        }],
        "no-undef": "off",
        "no-extra-boolean-cast": "error",
        "no-extra-semi": "error"
    }
}
